  </header>
  <main>
    <section class="banner">
      <div class="areaForm">
        <form action="#" class="form1">
          <div class="pass">
            <h2>PASSAGENS AÉREAS</h2>
            <input type="button" name="btnIda" id="ida" value="Somente ida">
            <input type="button" name="btnIdaVolta" id="idaVolta" value="Ida e volta">
          </div>
          <div class="geral">
            <div class="locals">
              <div>
                <h6>De onde você está saindo?</h6>
                <label for="origem"></label>
                <div class="input">
                  <i class="fas fa-plane-departure" style="color: #474c53;"></i>
                  <input type="text" name="origem" id="origem" placeholder="Busque por aeroportos">
                </div>
              </div>
              <div class="decoration">
                <button><i class="fas fa-exchange-alt" style="color: #ec8a08;"></i></button>
              </div>
              <div>
                <h6>Para onde você vai?</h6>
                <div class="input">
                  <label for="destino"></label>
                  <i class="fas fa-plane-arrival" style="color: #474c53;"></i>
                  <input type="text" name="destino" id="destino" placeholder="Busque por aeroportos">
                </div>
              </div>
            </div>
            <div class="dates">
              <div>
                <h6>Escolha as datas</h6>
                <label for="ida"></label>
                <input type="date" name="ida" id="ida">
                <label for="volta"></label>
                <input type="date" name="volta" id="volta">
              </div>
            </div>
            <div class="travelers">
              <div>
                <h6>passageiros</h6>
                <div>
                  <label for="passageiros"></label>
                  <input type="text" name="passageiros" id="passageiros" min="1" max="10" placeholder="Passageiros">
                </div>
              </div>
              <button type="submit">BUSCAR VOOS</button>
            </div>
          </div>
        </form>
      </div>
    </section>
    <section class="ads">
      <div class="ads1">
        <img src="/images/banner.png" alt="adsBanner">
      </div>
      <div class="ads2">
        <img src="#" alt="">
      </div class="ads2">
